{
  "manifest_version": 2,

  "name": "Seamless DOH Grades",
  "description": "See NYC Dept of Health inspection grades and violations alongside your favorite restaurants on seamless",
  "version": "1.0",

  "content_scripts": [
    {
      "matches": ["http://www.seamless.com/food-delivery/vendors*"],
      "js": ["jquery.js", "insert_grades.js"],
      "run_at": "document_start"
    }
  ],
  "permissions": [
    "https://seamless.com/*", "http://seamless-health-grades.herokuapp.com/*"
  ],
  "web_accessible_resources": [
    "*.jpg", "*.gif"
  ],
  "icons": {
    "128": "images/extension_icon_2.png"
  }
}
